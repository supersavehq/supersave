{
  "name": "supersave",
  "version": "0.17.0",
  "description": "A package to create a simple datastore with a generic API for side projects.",
  "main": "build/index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "concurrently --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check --loglevel=warn src",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext=js,jsx,ts,tsx --max-warnings=0 src",
    "format": "prettier --write src && eslint --ext=js,jsx,ts,tsx --fix src",
    "test": "jest",
    "test:mysql": "cross-env CONN=mysql://supersave:savesuper@127.0.0.1:3306/supersave jest --runInBand --forceExit",
    "test:watch": "cross-env DEBUG=supersave:* jest --watch",
    "test:watch:mysql": "cross-env DEBUG=supersave:* CONN=mysql://supersave:savesuper@127.0.0.1:3306/supersave jest --runInBand --forceExit --watch",
    "clean": "rimraf build",
    "start": "nodemon tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supersavehq/supersave.git"
  },
  "types": "./build/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "sqlite",
    "database",
    "store",
    "api",
    "headless",
    "mysql",
    "supersave",
    "super"
  ],
  "author": "debuggingdan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supersavehq/supersave/issues"
  },
  "homepage": "https://github.com/supersavehq/supersave#readme",
  "prettier": "eslint-config-daangemist/.prettierrc",
  "devDependencies": {
    "@tsconfig/node14": "14.1.4",
    "@types/debug": "^4.1.12",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.0.6",
    "@types/pluralize": "0.0.33",
    "@types/slug": "5.0.9",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "concurrently": "9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-debuggingdan": "2.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^40.0.0",
    "express": "5.1.0",
    "husky": "9.1.7",
    "jest": "30.0.3",
    "mysql2": "^3.14.1",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "sqlite": "5.1.1",
    "sqlite3": "^5.1.7",
    "superagent": "10.2.1",
    "supertest": "7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "express": "^4.17.0 || ^5.0.0",
    "mysql2": "^3.11.0 || ^3.14.1",
    "sqlite": "^4.0 || ^5.0",
    "sqlite3": "^5.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "sqlite": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.4.1",
    "pluralize": "^8.0.0",
    "short-uuid": "5.2.0",
    "slug": "11.0.0"
  }
}
