{
  "name": "supersave",
  "version": "0.11.0",
  "description": "A package to create a simple datastore with generic API for side projects.",
  "main": "build/index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "test": "jest",
    "test:mysql": "jest --runInBand --forceExit",
    "test:watch": "cross-env DEBUG=supersave:* jest --watch",
    "test:watch:mysql": "cross-env DEBUG=supersave:* jest --runInBand --forceExit --watch",
    "clean": "rimraf build",
    "start": "nodemon tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangemist/supersave.git"
  },
  "types": "./build/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "sqlite",
    "database",
    "store",
    "api",
    "headless",
    "mysql",
    "supersave",
    "super"
  ],
  "author": "daangemist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daangemist/supersave/issues"
  },
  "homepage": "https://github.com/daangemist/supersave#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/mysql": "^2.15.19",
    "@types/node": "^14.17.27",
    "@types/pluralize": "0.0.29",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-unicorn": "^37.0.1",
    "express": "^4.17.1",
    "husky": "^4.3.6",
    "jest": "^27.3.1",
    "mysql": "^2.18.1",
    "mysql-parse": "^2.0.8",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "sqlite": "^4.0.23",
    "sqlite3": "^4.2.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "express": "~4.17",
    "mysql": "~2.18",
    "sqlite": "~4.0",
    "sqlite3": "~5.0"
  },
  "dependencies": {
    "debug": "^4.3.2",
    "pluralize": "^8.0.0",
    "short-uuid": "^4.2.0",
    "slug": "^4.1.0"
  }
}
