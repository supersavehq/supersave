{
  "name": "supersave",
  "version": "0.9.1",
  "description": "A package to create a simple datastore with generic API for side projects.",
  "main": "build/index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "eslint ./src",
    "lint-fix": "eslint --fix ./src",
    "test": "jest",
    "test-mysql": "jest --runInBand",
    "test-watch": "cross-env DEBUG=supersave:* jest --watch",
    "test-watch-mysql": "cross-env DEBUG=supersave:* jest --runInBand --watch",
    "clean": "rimraf build",
    "start": "nodemon tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangemist/supersave.git"
  },
  "types": "./build/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "sqlite",
    "database",
    "store",
    "api",
    "headless",
    "mysql",
    "supersave",
    "super"
  ],
  "author": "daangemist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daangemist/supersave/issues"
  },
  "homepage": "https://github.com/daangemist/supersave#readme",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/mysql": "^2.15.18",
    "@types/node": "^14.14.17",
    "@types/pluralize": "0.0.29",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "express": "^4.17.1",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "mysql": "^2.18.1",
    "mysql-parse": "^2.0.8",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "sqlite": "^4.0.22",
    "sqlite3": "^5.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.2"
  },
  "peerDependencies": {
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "sqlite": "^4.0.22",
    "sqlite3": "^5.0.2"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "pluralize": "^8.0.0",
    "short-uuid": "^4.1.0",
    "slug": "^4.1.0"
  }
}
