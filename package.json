{
  "name": "supersave",
  "version": "0.15.0",
  "description": "A package to create a simple datastore with a generic API for side projects.",
  "main": "build/index.js",
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "concurrently --kill-others-on-fail \"npm:lint:*\"",
    "lint:prettier": "prettier --check --loglevel=warn src",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint --ext=js,jsx,ts,tsx --max-warnings=0 src",
    "format": "prettier --write src && eslint --ext=js,jsx,ts,tsx --fix src",
    "test": "jest",
    "test:mysql": "cross-env CONN=mysql://supersave:savesuper@127.0.0.1:3306/supersave jest --runInBand --forceExit",
    "test:watch": "cross-env DEBUG=supersave:* jest --watch",
    "test:watch:mysql": "cross-env DEBUG=supersave:* CONN=mysql://supersave:savesuper@127.0.0.1:3306/supersave jest --runInBand --forceExit --watch",
    "clean": "rimraf build",
    "start": "nodemon tests/run.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangemist/supersave.git"
  },
  "types": "./build/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "sqlite",
    "database",
    "store",
    "api",
    "headless",
    "mysql",
    "supersave",
    "super"
  ],
  "author": "daangemist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daangemist/supersave/issues"
  },
  "homepage": "https://github.com/daangemist/supersave#readme",
  "prettier": "eslint-config-daangemist/.prettierrc",
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.17.27",
    "@types/pluralize": "0.0.29",
    "@types/slug": "^0.9.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "eslint-config-daangemist": "^1.0.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-unicorn": "^10.0.0",
    "express": "^4.17.1",
    "husky": "^4.3.6",
    "jest": "^29.5.0",
    "mysql2": "^3.11.4",
    "nodemon": "^3.1.7",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.1.7",
    "superagent": "^7.1.6",
    "supertest": "^6.1.6",
    "ts-jest": "^29.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "express": "^4.17.0 || ^5.0.0",
    "mysql2": "^3.11.0",
    "sqlite": "^4.0",
    "sqlite3": "^5.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "sqlite": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.3.2",
    "pluralize": "^8.0.0",
    "short-uuid": "^4.2.0",
    "slug": "^4.1.0"
  }
}
